[Unit]
Description = Pelican service osdf-origin with multiuser support
After = network.target nss-lookup.target
Conflicts = osdf-origin.service

[Service]
ExecStart = /usr/bin/osdf --config /etc/pelican/osdf-origin.yaml origin serve
CapabilityBoundingSet = CAP_SETUID CAP_SETGID CAP_DAC_OVERRIDE

[Install]
WantedBy = multi-user.target
