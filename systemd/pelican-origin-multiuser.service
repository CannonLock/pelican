[Unit]
Description = Pelican service pelican-origin with multiuser support
After = network.target nss-lookup.target
Conflicts = pelican-origin.service

[Service]
ExecStart = /usr/bin/pelican --config /etc/pelican/pelican-origin.yaml origin serve
CapabilityBoundingSet = CAP_SETUID CAP_SETGID CAP_DAC_OVERRIDE

[Install]
WantedBy = multi-user.target
