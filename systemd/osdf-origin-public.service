[Unit]
Description = Pelican service osdf-origin, public instance
After = network.target nss-lookup.target

[Service]
ExecStart = /usr/bin/osdf --config /etc/pelican/osdf-origin-public.yaml origin serve
Restart = on-failure
RestartSec = 20s
WorkingDirectory = /var/spool/osdf

[Install]
WantedBy = multi-user.target
